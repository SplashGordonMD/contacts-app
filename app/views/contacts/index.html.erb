 <h1>Casey's Contacts</h1>
 
 <% @contacts.each do |contact| %>
    <h3>Full_name: <%= contact.full_name %></h3>
    <h3>Email: <%= contact.email %></h3>
    <h3>Phone_number: <%= contact.Phone_number.japan_phone_number %></h3>  
<% end %>

<h2><%= link_to contact.full_name, "/contacts/#{{contact.id}" %></h2>